{
  "name": "node-slate",
  "version": "1.9.6",
  "description": "Node port of lord/slate",
  "license": "Apache-2.0",
  "main": "build/index.html",
  "repository": "github:center-key/node-slate",
  "homepage": "https://node-slate.js.org",
  "bugs": "https://github.com/center-key/node-slate/issues",
  "docs": "https://github.com/center-key/node-slate#readme",
  "author": "Various",
  "keywords": [
    "api-documentation",
    "api",
    "nodejs",
    "slate",
    "static-site-generator"
  ],
  "jshintConfig": {
    "esversion": 9,
    "strict": "implied",
    "eqeqeq": true,
    "node": true,
    "undef": true,
    "unused": true
  },
  "scripts": {
    "test": "jshint gulpfile.js",
    "prestart": "gulp build-uncompressed",
    "start": "gulp serve",
    "prebuild": "gulp clean",
    "build": "gulp build-static-site",
    "pub": "rm -rf ./../public/nlp.studio && mv ./build ./../public/nlp.studio && cd ./../public && git add . && git commit -m 'nlp.studio documentation' && git push",
    "deploy": "deploy.sh",
    "backup_postbuild": "gulp lint && gulp publish"
  },
  "dependencies": {
    "browser-sync": "~2.26",
    "del": "~5.1",
    "fuse.js": "~6.2",
    "gulp": "~4.0",
    "gulp-clean-css": "~4.3",
    "gulp-cli": "~2.3",
    "gulp-concat": "~2.6",
    "gulp-ejs": "~5.1",
    "gulp-htmlhint": "~3.0",
    "gulp-if": "~3.0",
    "gulp-jshint": "~2.1",
    "gulp-prettify": "~0.5",
    "gulp-rename": "~2.0",
    "gulp-sass": "~4.1",
    "gulp-uglify": "~3.0",
    "gulp-w3c-html-validator": "~2.0",
    "highlight.js": "~10.1",
    "imagesloaded": "~4.1",
    "jquery": "~3.5",
    "jquery-highlight": "~3.5",
    "jquery-ui": "~1.12",
    "jquery-ui-dist": "~1.12",
    "jquery.tocify": "~1.9",
    "js-yaml": "~3.14",
    "jshint": "~2.11",
    "marked": "~1.1",
    "merge-stream": "~2.0",
    "node-sass": "~4.14",
    "tocbot": "~4.11"
  }
}
